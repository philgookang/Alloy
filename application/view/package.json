{
  "name": "view",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "make": "npm run make-dev && npm run make-min && npm run make-table",
    "make-dev": "browserify -t [ envify --NODE_ENV development ] src/react-bundle.js > ../../public/js/generate/react-bundle.js",
    "make-min": "browserify -t [ envify --NODE_ENV production ] src/react-bundle.js | uglifyjs > ../../public/js/generate/react-bundle.min.js",
    "make-table": "babel --presets react src/table.js > ../../public/js/generate/table.js"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-react": "^6.3.13",
    "browserify": "^12.0.1",
    "envify": "^3.4.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "uglifyjs": "^2.4.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  }
}
